{
  "logLevel": {
    "console": "error",
    "file": "debug"
  },
  "logDir": "./logs",

  "workspacePaths": ["/path/to/your/workspace"],

  "models": [
    {
      "provider": "ChatOpenAI",
      "args": {
        "model": "gpt-4o",
        "temperature": 0.1
      },
      "useForEvaluation": true # optional
    }
  ],

  "jdtlsBinaryPath": "./vendor/jdtls/bin/jdtls",
  "jdtlsBundles": ["./vendor/jdtls/java-analyzer-bundle/java-analyzer-bundle.core/target/java-analyzer-bundle.core-1.0.0-SNAPSHOT.jar"],
  "jvmMaxMem": "4g",

  "kaiAnalyzerRpcPath": "./vendor/analyzer/kai_analyzer_rpc",
  "rulesPaths": ["./vendor/rulesets"],
  "targets": ["quarkus", "cloud-readiness"],
  "sources": [],

  "solutionServerUrl": "http://localhost:8080"
}