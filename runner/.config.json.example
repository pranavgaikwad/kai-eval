{
  "logLevel": "info",
  "logDir": "./logs",

  "workspacePaths": ["/path/to/your/workspace"],

  "modelProvider": "ChatOpenAI",
  "modelArgs": {
    "model": "gpt-4o",
    "temperature": 0.1
  },

  "jdtlsBinaryPath": "/path/to/jdtls/bin/jdtls",
  "jdtlsBundles": ["/path/to/bundle.jar"],
  "jvmMaxMem": "4g",

  "kaiAnalyzerRpcPath": "/path/to/kai/analyzer/rpc/binary",
  "rulesPaths": ["/path/to/rule.yaml", "/path/to/rulesets/"],
  "targets": ["target1", "target2"],
  "sources": ["source1", "source2"],

  "solutionServerUrl": "http://localhost:8080",
}